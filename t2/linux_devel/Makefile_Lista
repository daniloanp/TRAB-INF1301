CC=gcc
CFLAGS=-I. -I../headers -L../lib -O0 -fpermissive
+CFLAGS=-Wall

all: lista
lista: 
	$(CC) $(CFLAGS) -c -x c++ LISTA.c -o ../obj/LISTA.o

teste-lista: lista
	$(CC) $(CFLAGS) -c -x c++ TESTLIS.c -o ../obj/TESTLIS.o
	$(CC) $(CFLAGS) -o ../bin/teste_lista.exe ../obj/LISTA.o ../obj/TESTLIS.o -lArcaboucoTeste

clean:
	rm *.o
	rm *.exe
